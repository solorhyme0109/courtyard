{
  "appName": "webpaas",
  "title": "webpaas-test",
  "theme": "dark",
  "themesData": {
    "dark": {
      "primaryColor": "#000"
    },
    "light": {
      "primaryColor": "#fff"
    }
  },
  "pages": [
    {
      "route": "/",
      "pageId": "/",
      "title": "index",
      "components": [
        {
          "id": "packages/bom/components/Text",
          "src": "packages/bom/components/Text",
          "component": "Text",
          "props": {
            "content": "Hello MyWebPaas"
          }
        },
        {
          "id": "packages/bom/components/Input",
          "src": "packages/bom/components/Input",
          "component": "Input",
          "props": {
            "value": "@store.username"
          },
          "mutation": {
            "onChange": "@store.username"
          }
        },
        {
          "id": "packages/bom/components/Button",
          "src": "packages/bom/components/Button",
          "component": "Button",
          "props": {
            "desc": "按钮"
          },
          "effects": {
            "alert": {
              "message": "submmiting"
            },
            "request": {
              "url": "api.domian.com/login",
              "body": {
                "username": "@store.username",
                "lvl": "@query.lvl",
                "userId": "@match.userId"
              }
            }
          }
        }
      ],
      "store": {
        "username": "annoying"
      },
      "style": {

      },
      "pages": [
        {
          "route": "/console",
          "pageId": "/console",
          "title": "控制台",
          "template": [],
          "store": {},
          "style": {}
        }
      ]
    }
  ]
}
